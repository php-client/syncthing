{
    "name": "php-client/syncthing",
    "description": "PHP Client for Syncthing API",
    "keywords": ["syncthing", "api", "client", "saloon"],
    "type": "library",
    "require": {
        "php": "^8.3",
        "saloonphp/saloon": "^3.9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpClient\\Syncthing\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "symfony/var-dumper": "^7.1",
        "pestphp/pest": "^2.34",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
